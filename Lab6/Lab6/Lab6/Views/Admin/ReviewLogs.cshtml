@model Lab6.Models.Admin.ReviewLogModel

<h2>Review logs</h2>

<table class="table">
    <tbody>
    <tr>
        <th>
            Review id
        </th>
        <th>
            User id
        </th>
        <th>
            event
        </th>
    </tr>

    @foreach (var log in Model.ReviewLogs)
    {
        <tr>
            <th>
                <a asp-controller="Announcement" asp-action="AnnouncementItem" asp-route-id="@log.AnnouncementId">
                    @log.ReviewId
                </a>
            </th>
            <th>
                <a asp-controller="User" asp-action="UserProfile" asp-route-id="@log.UserId">
                    @log.UserId
                </a>
            </th>
            <th>
                @log.Event
            </th>
        </tr>
    }
</table>